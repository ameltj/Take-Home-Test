#include <stdio.h>

int main () {
		int x;
		scanf ("%d", &x);
		if ( x >= 0 && x <= 9){
			printf ("1");
		}
		else if ( x >= 10 && x <= 99){
			printf ("2");
		}
		else if ( x >= 100 && x <= 999){
			printf ("3");
		}
		else if ( x >= 1000 && x <= 1000000){
			printf ("More than 3 digits");
		}
	
	return 0;
}
