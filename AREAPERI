#include <stdio.h>

int main () {
    int L;
	  int B;
	  scanf ("%d", &L);
	  scanf ("%d", &B);
	
    if ( L*B > 2*(L+B)) {
	  	printf ("Area\n");
		  printf ("%d", L*B);
	  }
	  else if (L*B < 2*(L+B)) {
		  printf ("Peri\n");
		  printf ("%d", 2*(L+B));
  	}
  	else {
      printf ("Eq\n");
		  printf ("%d", L*B);
	  }

	return 0;
}
